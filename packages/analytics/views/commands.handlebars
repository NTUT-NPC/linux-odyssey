<h1>Commands</h1>
<div>
  <h2>Error commands</h2>
  <div class='pagination'>
    <a href='/commands?order=asc'>Ascending</a>
    <a href='/commands?order=desc'>Descending</a>
  </div>
  <table>
    <tr>
      <th>Command</th>
      <th>User</th>
      <th>Quest</th>
      <th>Session</th>
      <th>Error</th>
      <th>Created At</th>
    </tr>
    {{#each errorCommands}}
      <tr>
        <td>{{command}}</td>
        <td>{{user}}</td>
        <td>{{quest}}</td>
        <td><a href='/sessions/{{session}}'>Detail</a></td>
        <td>{{error}}</td>
        <td>{{createdAt}}</td>
      </tr>
    {{/each}}
  </table>
  <div class='pagination'>
    <a href='/commands?nextKey={{nextKey}}&order={{order}}'>Next</a>
  </div>
</div>