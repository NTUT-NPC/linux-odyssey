<h1>Users List</h1>
<div class='pagination'>
  <a href='/users?order=asc'>Ascending</a>
  <a href='/users?order=desc'>Descending</a>
</div>
<div>
  <table>
    <tr>
      <th>Username</th>
      <th>Last Activity</th>
      <th>Sessions Count</th>
      <th>Email</th>
      <th>Registration Time</th>
      <th>Registration Method</th>
    </tr>
    {{#each users}}
      <tr>
        <td href='/users/{{_id}}'>{{username}}</td>
        <td>{{lastActivityAt}}</td>
        <td>{{sessionCount}}</td>
        <td>{{email}}</td>
        <td>{{createdAt}}</td>
        <td>{{method}}</td>
      </tr>
    {{/each}}
  </table>
</div>
<div class='pagination'>
  <a href='/users?nextKey={{nextKey}}&order={{order}}'>Next</a>
</div>