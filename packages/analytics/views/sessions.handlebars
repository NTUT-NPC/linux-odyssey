<h1>Sessions</h1>
<div>
  <table>
    <tr>
      <th>User</th>
      <th>Quest</th>
      <th>Status</th>
      <th>Created At</th>
      <th>Finished At</th>
      <th>Used Time</th>
      <th>Command Count</th>
      <th>Session</th>
    </tr>
    {{#each sessions}}
      <tr>
        <td>{{user}}</td>
        <td>{{quest}}</td>
        <td>{{status}}</td>
        <td>{{createdAt}}</td>
        <td>{{finishedAt}}</td>
        <td>{{usedTime}}</td>
        <td>{{commandCount}}</td>
        <td><a href='/sessions/{{_id}}'>Detail</a></td>
      </tr>
    {{/each}}
  </table>
</div>
<div class='pagination'>
  {{#each pages}}
    <a href='/sessions?page={{this}}'>{{this}}</a>
  {{/each}}
</div>